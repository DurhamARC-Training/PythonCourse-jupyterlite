{
  "LiteBuildConfig": {
    "ignore_contents": [
      ".git",
      ".jupyter",
      ".vscode",
      ".ipynb_checkpoints",
      "__pycache__",
      "notebook_settings.json",
      "environment.yml",
      "README.md",
      "pull_files_*.py"
    ],
    "extra_file_types": {
      "mplstyle": {
        "name": "mplstyle",
        "extensions": [".mplstyle"],
        "fileFormat": "text",
        "mimeTypes": ["text/plain"]
      },
      "xyz": {
        "extensions": [".xyz"],
        "fileFormat": "text",
        "mimeTypes": ["text/plain"],
        "name": "xyz"
      }
    }
  }
}