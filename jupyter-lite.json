{
  "jupyter-lite-schema-version": 0,
  "jupyter-config-data": {
    "disabledExtensions": [
      "@jupyterlab/drawio-extension",
      "jupyterlab-kernel-spy",
      "jupyterlab-tour"
    ],
    "appName": "jupyterlab-deck",
    "exposeAppInBrowser": true,
    "ContentsManager": {
      "allow_hidden": true,
      "hide_globs": [
        "startup",
        "requirements.txt",
        "__pycache__",
        "notebook_settings.json",
        "environment.yml",
        "README.md",
        "pull_files_*.py"
      ]
    }
  }
}
  